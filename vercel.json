{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31556952, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path((?!zh-CN|en-US|ja-JP|zh-TW).*).html",
      "destination": "/zh-CN/:path.html"
    },
    {
      "source": "/:path((?!zh-CN|en-US|ja-JP|zh-TW).*)/",
      "destination": "/zh-CN/:path/"
    },
    {
      "source": "/",
      "destination": "/zh-CN/"
    }
  ],
  "github": {
    "silent": true
  }
}
